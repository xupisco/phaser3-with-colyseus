{
    "compilerOptions": {
        "target": "ES5",
        "module": "commonjs",
        "sourceMap": true,
        "outDir": "dist",
        "allowJs": true,
        "strictNullChecks": false,
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "lib": [
            "es5",
            "dom",
            "scripthost"
        ]
    }
}
